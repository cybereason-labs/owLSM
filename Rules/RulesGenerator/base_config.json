{
    "features": {
        "file_monitoring": {
            "enabled": true,
            "events": {
                "chmod": true,
                "chown": true,
                "file_create": true,
                "unlink": true,
                "rename": true,
                "write": true,
                "read": true,
                "mkdir": true,
                "rmdir": true
            }
        },
        "network_monitoring": {
            "enabled": true
        }
    },
    "userspace": {
        "max_events_queue_size": 10000,
        "output_type": "JSON",
        "log_level": "LOG_LEVEL_INFO",
        "set_limits": true
    },
    "kernel": {
        "log_level": "LOG_LEVEL_WARNING"
    }
}